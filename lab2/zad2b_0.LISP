(defun broj_p(br lista)
       (cond((atom br)
                 (cond((null lista) '0)
                      ((and(atom(car lista))(equal br (car lista)) ) (+ 1 (broj_p br (cdr lista))))
                      ((and(atom(car lista))(not(equal br (car lista))) ) (+ 0 (broj_p br (cdr lista))))
                      ((listp(car lista))  (+ (broj_p br (car lista)) (broj_p br (cdr lista))))
                 )
            )
            ((listp br)
                 (cond((null lista) '0)
                      ((atom(car lista)) (+ 0 (broj_p br (cdr lista))))
                      ((and(listp(car lista)) (equal br (car lista)) ) (+ 1 (broj_p br (cdr lista))))
                      ((listp(car lista)) (+ (broj_p br (car lista)) (broj_p br (cdr lista))))
                 )
            )
       )
)
